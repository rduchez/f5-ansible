when HTTP_REQUEST {
  set reqtime [clock clicks]
}
when HTTP_RESPONSE {
  ISTATS::set "sys.application.service <APP> string app-rtt" [expr {[clock clicks] - $reqtime}]
  ISTATS::set "sys.application.service <APP> string tcp-rtt" [TCP::rtt]
}
    }

